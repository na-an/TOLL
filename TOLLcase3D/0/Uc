FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Uc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    INLET1
    {
        type            fixedValue;
        value           uniform (5e-3 0 0);
    }
    INLET2
    {
        type            fixedValue;
        value           uniform (5e-3 0 0);
    }
    OUTLET
    {
        type            adjointOutletVelocityPower;
        value           uniform (0 0 0);
    }
    ALLWALL
    {
        type            noSlip;
    }
    SYMSURFACE
    {
        type            symmetry;
    }
}


