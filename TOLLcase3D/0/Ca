FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ca;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 3;

boundaryField
{
    INLET1
    {
        type            fixedValue;
        value           uniform 0 ;
    }
    INLET2
    {
        type            fixedValue;
        value           uniform 0 ;
    }
    OUTLET
    {
        type            CaOutlet;
        value           uniform 0 ;
    }
    ALLWALL
    {
        type            zeroGradient;
    }
    SYMSURFACE
    {
        type            symmetry;
    }
}


