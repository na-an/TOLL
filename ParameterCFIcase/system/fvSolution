solvers
{
     "(adalpha|alphac)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0;
    }
    "(p_rgh|pa|p|pc)"
    {
        solver          GAMG;
        tolerance       1e-9;
        relTol          0.01;
        smoother        GaussSeidel;
    }

    "(p_rghFinal|paFinal|pcFinal)"
    {
        $p_rgh;
        relTol          0;
    }

    "(U|Ua|Uc)"
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        tolerance       1e-7;
        relTol          0.1;
        nSweeps         2;
    }
    "(fsensPDE|gsensPDE|dvgPDE)"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        relTol          0;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    pRefCell 0;
    pRefValue 0;

}

